{
  "version": 2,
  "builds": [
    {
      "src": "apps/api/app/main.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/api/app/main.py"
    }
  ],
  "env": {
    "REDIS_URL": "@redis_url",
    "DATABASE_URL": "@database_url"
  }
}
